{
  "name": "execution-timer",
  "displayName": "Execution Timer",
  "description": "A VS Code extension to track and display program runtime in seconds in the status bar",
  "version": "0.0.1",
  "publisher": "yyhaos",
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onDebug",
    "onCommand:extension.resetTimer",
    "onDebugChangeActiveDebugSession",
    "onDidChangeBreakpoints",
    "onDebugTerminate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.resetTimer",
        "title": "Reset Timer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "typescript": "^4.0.3",
    "undici": "^4.16.0",
    "vsce": "^2.15.0",
    "vscode": "^1.1.37",
    "web-streams-polyfill": "^4.0.0"
  }
}